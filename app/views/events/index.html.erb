<table>
  <% @events.each do |event| %>
    <tr>
      <td><%= event.name %></td>
      <td><%= link_to 'show', event_path(event) %></td>
      <!-- <td><%= link_to 'XML', event_path(event, :format => :xml) %></td> -->
      <!-- <td><%= link_to 'JSON', event_path(event, :format => :json) %></td> -->
      <td><%= link_to 'edit', edit_event_path(event) %></td>
      <td><%= button_to 'delete', event_path(event), :method => :delete %></td>
  <%#= link_to 'Show', :controller => 'events', :action => 'show', :id => event %>
  <%#= link_to 'Edit', :controller => 'events', :action => 'edit', :id => event %>
  <%#= link_to 'Delete', :controller => 'events', :action => 'destroy', :id => event %>
    </tr>
  <% end %>
</table>
<%= button_to 'new event', new_event_path %>
<%#= link_to 'New event', :controller => 'events', :action => 'new' %>
<br>
<%= paginate @events %>